        <Label x:Name='labStatus' Content="Status klienta:" HorizontalAlignment="Left" Height="32" Margin="33,349,0,0" VerticalAlignment="Top" Width="109" FontSize="14" />
        <Label x:Name='labStatus_Copy' Content="Uwagi:" HorizontalAlignment="Left" Height="32" Margin="33,386,0,0" VerticalAlignment="Top" Width="109" FontSize="14" />
        <TextBox x:Name="txtNazwa" HorizontalAlignment="Left" Height="32" Margin="147,16,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="465"/>
        <TextBox x:Name="txtNIP" HorizontalAlignment="Left" Height="32" Margin="147,90,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="465"/>
        <TextBox x:Name="txtDataZal" HorizontalAlignment="Left" Height="32" Margin="147,127,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="465"/>
        <TextBox x:Name="txtKraj" HorizontalAlignment="Left" Height="32" Margin="147,164,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="465"/>
        <TextBox x:Name="txtMiasto" HorizontalAlignment="Left" Height="32" Margin="147,201,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="465"/>
        <TextBox x:Name="txtAdres" HorizontalAlignment="Left" Height="32" Margin="147,238,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="465"/>
        <TextBox x:Name="txtKodPocz" HorizontalAlignment="Left" Height="32" Margin="147,275,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="465"/>
        <TextBox x:Name="txtOpis" HorizontalAlignment="Left" Height="32" Margin="147,312,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="465"/>
