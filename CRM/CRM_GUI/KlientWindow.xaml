            <ComboBoxItem Content="Elektronika" HorizontalAlignment="Left" Width="463.2"/>
            <ComboBoxItem Content="Telekomunikacja" HorizontalAlignment="Left" Width="463.2"/>
            <ComboBoxItem Content="Motoryzacja" HorizontalAlignment="Left" Width="463.2"/>
            <ComboBoxItem Content="Media" HorizontalAlignment="Left" Width="463.2"/>
            <ComboBoxItem Content="Energetyka" HorizontalAlignment="Left" Width="463.2"/>
            <ComboBoxItem Content="Inne" HorizontalAlignment="Left" Width="463.2"/>
        </ComboBox>
        <ComboBox x:Name="cmboxStatus" HorizontalAlignment="Left" Height="32" Margin="147,349,0,0" VerticalAlignment="Top" Width="465">
            <ComboBoxItem Content="Stały" HorizontalAlignment="Left" Width="463.2"/>
            <ComboBoxItem Content="Były" HorizontalAlignment="Left" Width="463.2"/>
            <ComboBoxItem Content="Nowy" HorizontalAlignment="Left" Width="463.2"/>
            <ComboBoxItem Content="Potencjalny" HorizontalAlignment="Left" Width="463.2"/>
