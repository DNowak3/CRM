        <Button x:Name="buttonAnuluj" Content="Anuluj" HorizontalAlignment="Left" Height="32" Margin="604,254,0,0" VerticalAlignment="Top" Width="93" Click="buttonAnuluj_Click" />
        <ComboBox x:Name="cmboxPracownik" HorizontalAlignment="Left" Height="32" Margin="177,129,0,0" VerticalAlignment="Top" Width="520">
        </ComboBox>
        <ComboBox x:Name="cmbokWynik" HorizontalAlignment="Left" Height="32" Margin="177,166,0,0" VerticalAlignment="Top" Width="520">
