        xmlns:local="clr-namespace:CRM_GUI"
        mc:Ignorable="d"
        Title="DzialanieWindow" Height="411.307" Width="761.306">
    <Grid Margin="0,0,4.2,0" Height="381" VerticalAlignment="Top">
