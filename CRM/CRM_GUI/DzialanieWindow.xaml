        <Button x:Name="buttonAnuluj" Content="Anuluj" HorizontalAlignment="Left" Height="32" Margin="604,179,0,0" VerticalAlignment="Top" Width="93" Click="buttonAnuluj_Click" />
        <ComboBox x:Name="cmbokWynik" HorizontalAlignment="Left" Height="32" Margin="177,91,0,0" VerticalAlignment="Top" Width="520">
           
        </ComboBox>

