            <ComboBoxItem Content="ukonczono" HorizontalAlignment="Left" Width="463.2"/>
            <ComboBoxItem Content="anulowano" HorizontalAlignment="Left" Width="463.2"/>
            <ComboBoxItem Content="zaplanowano" HorizontalAlignment="Left" Width="463.2"/>
